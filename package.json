{
  "name": "muj-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "init-assistant": "tsx scripts/init-assistant.ts",
    "build": "tsc --project tsconfig.json && prisma generate && next build",
    "vercel-build": "tsc --project tsconfig.json && prisma generate && node --loader ts-node/esm scripts/migrate-database.js && next build",
    "start": "next start",
    "lint": "next lint",
    "db:sync": "node scripts/sync-db.js",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push && node --loader ts-node/esm prisma/seed.ts",
    "db:migrate": "node --loader ts-node/esm scripts/migrate-database.js",
    "postinstall": "prisma generate",
    "backup": "node --loader ts-node/esm scripts/backup.ts",
    "restore": "node --loader ts-node/esm scripts/restore.ts",
    "export-products": "node --loader ts-node/esm scripts/export-products.js",
    "assistant-setup": "node --loader ts-node/esm scripts/export-products.js",
    "upload-screenshots": "node scripts/upload-screenshots.js",
    "fix-imageurl": "node --loader ts-node/esm scripts/fix-imageUrl-database.js",
    "check-screenshots": "node --loader ts-node/esm scripts/check-screenshot-status.js",
    "fix-screenshots": "node --loader ts-node/esm scripts/fix-missing-screenshots.js"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^5.15.7",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node-fetch": "^2.6.12",
    "@vercel/blob": "^1.1.1",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "dotenv": "^16.5.0",
    "form-data": "^4.0.2",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "node-fetch": "^3.3.2",
    "openai": "^4.100.0",
    "puppeteer": "^24.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.2",
    "stripe": "^18.2.1",
    "tailwind-merge": "^3.0.1",
    "typed.js": "^2.1.0",
    "uuid": "^11.1.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.17.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.35",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3"
  }
